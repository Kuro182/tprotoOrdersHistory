                                                </Grid.RowDefinitions>

                                                <Grid Grid.Row="1">
                                                    <Button
                                                        Width="Auto"
                                                        Height="Auto"
                                                        Background="{DynamicResource GreenMainColor}"
                                                        BorderThickness="0" />
                                                </Grid>

                                            </Grid>

                                        </Grid>
                                    </Grid>
                                </TabItem>
                                <TabItem x:Name="Market3" Style="{DynamicResource StyleButton_id-2}">
                                    <TabItem.Header>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock
                                                Padding="3,5"
                                                Style="{StaticResource StyleTextBlock_id-1}"
                                                Text="Stop-Limit" />
                                        </StackPanel>
                                    </TabItem.Header>
                                    <Grid />
                                </TabItem>
                                <TabItem x:Name="Market4" Style="{DynamicResource StyleButton_id-2}">
                                    <TabItem.Header>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock
                                                Padding="3,5"
                                                Style="{StaticResource StyleTextBlock_id-1}"
                                                Text="Stop-Limit Profit" />
                                        </StackPanel>
                                    </TabItem.Header>
                                    <Grid />
                                </TabItem>
                            </TabControl>
                        </Grid>
                    </Grid>
                </Grid>
            </Grid>
        </Grid>

        <!--  Coin History  -->
        <Grid
            Grid.Row="2"
            Grid.Column="3"
            Margin="4.4,2.6,-1.4,0">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="7*" />
                </Grid.RowDefinitions>

                <Grid Grid.Row="1" Style="{StaticResource StyleColor_id-1}" />

                <Grid Grid.Row="0" Style="{StaticResource StyleColor_id-1_6}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0">
                        <TextBlock Style="{StaticResource StyleTextBlock_id-1}" Text="Price" />
                    </Grid>
                    <Grid Grid.Column="1">
                        <TextBlock Style="{StaticResource StyleTextBlock_id-1}" Text="CoinName" />
                    </Grid>
                    <Grid Grid.Column="2">
                        <TextBlock Style="{StaticResource StyleTextBlock_id-1}" Text="Time" />
                    </Grid>

                </Grid>
            </Grid>
        </Grid>



        <!--  Нижние панели  -->
        <Grid
            Grid.Row="3"
            Grid.Column="0"
            Grid.ColumnSpan="2"
            Margin="0,3,3,-0.4"
            Style="{StaticResource StyleColor_id-1_1}" />
        <Grid
            Grid.Row="3"
            Grid.Column="2"
            Grid.ColumnSpan="2"
            Margin="8,3,-1.4,-0.4"
            Style="{StaticResource StyleColor_id-1_1}" />

    </Grid>
</Window>




